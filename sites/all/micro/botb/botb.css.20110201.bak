h3#psaheader {
  padding-left:150px;
}

div#twtr-widget-1 {
  float:right;
}

#botbHeader {
    position: relative;
    height: 139px;
}

#botbNav {
    position: absolute;
    left: 0px;
    top: 0px;
}

#botbLogo a {
    background: url(bftb-logo_01.png) no-repeat;
    width: 300px;
    height: 326px;
    display: block;
    position: relative;
    bottom: 57px;
    right: 67px;
    text-indent: -9999em;
    outline: none;
}

#botbNav ul { margin: 0em; padding: 0em; }

#botbNav li {
    display: block;
    background: url(splat-trans.png) no-repeat;
    width: 200px;
    height: 188px;
    position: absolute;
}

#botbNav li a {
    display: block;
    width: 136px;
    height: 66px;
    text-indent: -9999em;
    outline: none;
    position: relative;
    top: 47px;
    left: 26px;
}

#botbNav li.start { left: 168px; top: -29px; }
#botbNav li.videos { left: 341px; top: -71px; }
#botbNav li.tips { left: 523px; top: -71px; }
#botbNav li.register { left: 699px; top: -3px; }

#botbNav li.start a { background: url(menu_24-trans.png) no-repeat -134px 0px; }
#botbNav li.videos a { background: url(menu_24-trans.png) no-repeat -133px -73px; }
#botbNav li.tips a { background: url(menu_24-trans.png) no-repeat -134px -198px; }
#botbNav li.register a { background: url(menu_24-trans.png) no-repeat -133px -132px; }

#botbNav li.start a:hover, #botbNav li.start-active a { background: url(menu_24-trans.png) no-repeat 0px 0px; }
#botbNav li.videos a:hover, #botbNav li.videos-active a { background: url(menu_24-trans.png) no-repeat -0px -73px; }
#botbNav li.tips a:hover, #botbNav li.tips-active a { background: url(menu_24-trans.png) no-repeat 0px -198px; }
#botbNav li.register a:hover, #botbNav li.register-active a { background: url(menu_24-trans.png) no-repeat -0px -132px; }

#botbMain {
    background: url(bg_24-trans.png) repeat-y; 
    width: 922px;
    margin-left: -38px;
    position: relative;
}

#botbTop {
    position: relative;
    background: url(bg_top_24-trans.png) no-repeat;
    width: 922px;
    height: 50px;
    margin-left: -38px;
}

.botbBody {
    left: 118px;
    top: 218px;
    width: 700px;
    color: #dddddd;
    padding: 20px 0 0px 120px;
}

#botbMain .botbBody p { color: #ccc; }
#botbMain a { color: #5588aa; }
.botbBody ol { font-size: 0.95em; color: #777799; }
.botbBody ul { font-size: 0.95em; color: #777799; }
.botbBody img.goright { padding-top: 0px; }
.botbBody h2 { color: #fafafa; }
.botbBody h3 { color: #e8e8e8; }

#edit-field-campaign-birthdate-timezone,
#edit-hs-form-build-id,
#edit-changed,
#edit-botb-node-form { display: none; }

.botbKit {
    border-style: dashed none;
    border-width: 1px;
    margin-bottom: 1em;
    padding: 1em 0;
    width: 500px;
}

.botbKit table tbody { border-top: none; }
.botbKit label, .botbKit input, .botbKit select, .botbKit textarea { display: block; }
.botbKitLeft { width: 280px; float: left; }
.botbKitRight { width: 200px; float: left; }
.botbKit span { display: inline; color: #777799; }
.botbKit span.form-required { color: red; }
.botbKit input, .botbKit select, .botbKit textarea {
    margin: 0px;
    padding: 3px 4px;
    border-color: #555555;
    border-style: none solid solid none;
    border-width: 1px;
}
.botbKit label { padding: 8px 0 2px; }
.botbKit input.signup { padding: 8px 0; border: medium none; }
.botbKit input.hidden{ display: none; }
.botbKitLeft input, .botbKitRight input { width: 192px; }

div#container { padding-bottom: 0px; }
div#wrapper { margin-bottom: 0em; }
div#footer { margin-bottom: 2em; margin-top: 70px; }
div#footer ul { margin-left: 80px; }
div#footer ul li { padding: 0 6px; font-size: 0.94em; }

/* list styles */
/*
.botbBody ol { margin-left: -0.5em; font-size: 1.9em;}
.botbBody ol li p { font-size: 0.5em; line-height: 1.4em; }
*/

/* entry pages */
.botbEntry .video { float: left; padding: 7px 15px 15px 0; }
#botbMain div.botbEntry p { font-size: 1.1em; color: #ddd; }
.botbEntry .question { display: block; font-size: 0.75em; font-weight: bold; color: #779; }
.botbEntry h2 span { font-size: 0.75em; font-weight: normal; }

/* rating system */

div.fivestar-widget-static .star {
  width: 25px;
  height: 25px;
  background: url(star.png) no-repeat 0 0px;
}

div.fivestar-widget-static .star span.on {
  background: url(star.png) no-repeat 0 -25px;
}

div.fivestar-widget-static .star span.off {
  background: url(star.png) no-repeat 0 0px;
}

/* Javascript Star Version */
div.fivestar-widget .cancel, div.fivestar-widget .star {
  width: 25px;
  height: 25px;
}
div.fivestar-widget .cancel, div.fivestar-widget .cancel a {
  background: url(cancel.png) no-repeat 0 0;
}

div.fivestar-widget .star, div.fivestar-widget .star a {
  background: url(star.png) no-repeat 0 0px;
}

div.fivestar-widget div.on a {
  background-position: 0 -25px;
}
div.fivestar-widget div.hover a, div.rating div a:hover {
  background-position: 0 -25px;
}

/* video gallery */

.video-teaser { width: 290px; float: left; padding: 5px 30px; }
.botb-video-row { clear: both; }
.botbBody .video-teaser h3 { margin: 1em 0 0.33em; }

.video-teaser img { float: left; padding: 0 15px 15px 0; }
#botbMain .video-teaser p { color: #ddd; margin-bottom: 0px; }

/* application */

.botb_node_form fieldset fieldset { 
    border: none;
    margin-left: 0px;
    padding-left: 0px;
    overflow: hidden;
}

.botb_node_form .group-your-song input { display: inline; }
.botb_node_form .group-official-contest-rules input { display: inline; }

/* edit pages */
div#botbMain form#views-edit-view { margin-left: 150px; color: white; }
div#botbMain form#views-edit-view fieldset { width: 650px; }
div#botbMain form#views-edit-view fieldset table { color: black; }

.question { display: block; font-weight: bold; font-size: 1.2em; }
.answer { display: block; padding-bottom: 1em; }
.clicktip { background-color: black; }
div#botbMain span.clicktip_target { text-decoration: none; color: #58A; font-style: normal; }
div#botbMain span.clicktip_target:hover { text-decoration: underline; }

div#winners h2 { color: #fec333; text-transform: uppercase; text-align: center; }
div#winners p { text-align: center; }
div#winners div.video { top: 20px; left: 280px; position: relative; z-index: 50; width: 400px; }
div#winner-splat { width: 400px; height: 350px; background: url(yellow_splat.png) no-repeat 0 0; position: absolute; left: -30px; top: 0px; }
#botbMain div#winner-splat p { height: 100px; margin: 0px; padding: 75px 0 0 100px; width: 176px; color: black; }

div#winners h3 { padding: 0px; margin: 0px; color: #fec333; font-size: 15px; }
div#winners h4 { padding: 0 0 4px; margin: 0px; color: #fec333; font-size: 14px; }
div.wvideo { width: 260px; float: left; padding: 0 45px; margin: 15px 0; }

div#botb-signup form#signup { text-align: center; }
div#botb-signup input#email { vertical-align: top; width: 180px; margin-top: 1px; height: 20px; }
